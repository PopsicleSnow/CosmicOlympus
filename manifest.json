{
    "name": "Custom Tab",
    "version": "1.0",
    "description": "Another take on a custom new tab",
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "permissions": [
        "storage"
    ],
    "browser_action": {
        "default_popup": "popup/popup.html",
        "default_icon": "images/favicons/collo.png"
    },
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
    "manifest_version": 2
}

